{
	"name": "froggy",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bash ./dev.sh",
		"dev:frontend": "vite dev",
		"dev:backend": "cd backend && npm run dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
