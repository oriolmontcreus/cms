		"mongoose": "^8.15.2",
		"redis": "^5.5.6",
		"svelte-sonner": "^1.0.5",
		"tailwindcss": "^4.1.9"
	},
	"devDependencies": {
		"@lucide/svelte": "^0.515.0",
		"@sveltejs/adapter-auto": "^6.0.0",
