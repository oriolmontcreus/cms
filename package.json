{
	"name": "froggy",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bash ./dev.sh",
		"dev:frontend": "vite dev",
		"dev:backend": "cd backend && npm run dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"create-user": "ts-node --project backend/scripts/tsconfig.json backend/scripts/create-user.ts"
	},
	"dependencies": {
		"axios": "^1.9.0",
		"bcrypt": "^5.1.1",
		"mongodb": "^6.3.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/typography": "^0.5.10",
		"@types/axios": "^0.14.0",
		"@types/bcrypt": "^5.0.2",
		"@types/node": "^20.11.0",
		"autoprefixer": "^10.4.17",
		"postcss": "^8.4.35",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.0.0",
		"vite": "^6.2.6"
	}
}
